<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.logappdev2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.supermegatron4000.view.activities.InstallerActivity"></activity>
        <activity android:name="com.example.supermegatron4000.view.activities.ActionActivity" />
        <activity android:name="com.example.supermegatron4000.view.activities.RoomActivity" />
        <activity
            android:name="com.example.supermegatron4000.view.activities.HomePage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.supermegatron4000.view.activities.LoginView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>